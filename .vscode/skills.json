{
  "$schema": "https://skillsmp.com/schemas/skills-v2.json",
  "version": "2.1",
  "workspace": {
    "name": "DMarket Telegram Bot",
    "description": "Telegram bot для торговли на DMarket с AI-модулями",
    "repository": "https://github.com/Dykij/DMarket-Telegram-Bot",
    "skills_directory": "src",
    "updated": "January 2026"
  },
  "discovery": {
    "auto_scan": true,
    "scan_patterns": [
      "src/**/SKILL_*.md",
      "src/**/marketplace.json"
    ],
    "ignore_patterns": [
      "**/node_modules/**",
      "**/__pycache__/**",
      "**/venv/**",
      "**/.pytest_cache/**"
    ]
  },
  "ai_assistants": {
    "enabled": true,
    "providers": [
      "github-copilot",
      "claude-code",
      "chatgpt",
      "cursor",
      "windsurf"
    ],
    "context_injection": {
      "enabled": true,
      "include_skill_docs": true,
      "include_examples": true,
      "include_dependencies": true,
      "include_performance_metrics": true
    },
    "experimental": {
      "vision_analysis": true,
      "multi_file_context": true,
      "semantic_search": true,
      "cross_skill_suggestions": true
    },
    "advanced_triggers": {
      "enabled": true,
      "file_patterns": {
        "enabled": true,
        "patterns": [
          {
            "pattern": "**/arbitrage*.py",
            "skills": ["ai-arbitrage-predictor"]
          },
          {
            "pattern": "**/ml/**/*.py",
            "skills": ["ensemble-builder", "advanced-feature-selector"]
          },
          {
            "pattern": "**/telegram_bot/handlers/**/*.py",
            "skills": ["nlp-command-handler"]
          }
        ]
      },
      "code_patterns": {
        "enabled": true,
        "patterns": [
          {
            "regex": "def\\s+(scan|predict)_\\w+\\(.*level.*\\)",
            "skills": ["ai-arbitrage-predictor"],
            "description": "Functions with level parameter suggest arbitrage scanning"
          },
          {
            "regex": "async\\s+def\\s+\\w+arbitrage\\w+",
            "skills": ["ai-arbitrage-predictor"],
            "description": "Async arbitrage functions"
          },
          {
            "regex": "sklearn|xgboost|RandomForest|GradientBoosting",
            "skills": ["ensemble-builder", "advanced-feature-selector"],
            "description": "ML model references"
          }
        ]
      },
      "comment_patterns": {
        "enabled": true,
        "patterns": [
          {
            "regex": "TODO.*arbitrage|FIXME.*prediction",
            "skills": ["ai-arbitrage-predictor"]
          },
          {
            "regex": "TODO.*ml|TODO.*model",
            "skills": ["ensemble-builder"]
          }
        ]
      },
      "context_aware": {
        "enabled": true,
        "in_function_like": ["scan_", "predict_", "analyze_", "process_"],
        "in_class_like": ["Predictor", "Scanner", "Analyzer", "Manager"]
      }
    }
  },
  "local": {
    "personal_mode": true,
    "skip_community_features": true,
    "enable_analytics": false,
    "enable_telemetry": false,
    "auto_backup": true,
    "backup_directory": ".backups/skills"
  },
